{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "builds": [
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    
    { "src": "/", "dest": "/public/index.html" },

    
    { "src": "/home", "dest": "/public/pages/home.html" },
    { "src": "/doctores", "dest": "/public/pages/doctores.html" },
    { "src": "/sensoformulario", "dest": "/public/pages/sensoformulario.html" },
    { "src": "/busqueda", "dest": "/public/pages/busqueda.html" },

   
    { "src": "/Pages/(.*)", "dest": "/public/pages/$1" },
    { "src": "/Img/(.*)", "dest": "/public/img/$1" },

  
    { "src": "/pages/(.*)", "dest": "/public/pages/$1" },
    { "src": "/img/(.*)", "dest": "/public/img/$1" }
  ]
}
